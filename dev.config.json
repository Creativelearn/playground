{
    "connectionString": {
        "name": "mongodb",
        "address": "127.0.0.1",
        "databaseName" : "playground2"
    },
    "scheduling": {
        "enabled": false
    },
    "phantom": {
        "numberOfWorkers" : 1,
        "timeout": 20000,
        "strategy": "phantom-server"
    },
    "electron": {
        "numberOfWorkers": 1,
        "strategy": "dedicated-process",
        "timeout": 40000
    },
    "authentication": {
        "enabled": false
    },
    "httpPort": 2500,
    "tasks": {
        "numberOfWorkers" : 1,
        "timeout": 10000,
        "strategy": "http-server",
        "nativeModules": [
            {
                "globalVariableName": "_",
                "module": "underscore"
            }
        ]
    },
    "templates": {
      "studio-link-button-visibility": false
    },
    "scripts": {
        "allowedModules": ["sendgrid", "feedparser", "sendgrid", "request", "underscore", "mongodb"]
    }
}
