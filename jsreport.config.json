{
  "httpPort": 5488,
  "allowLocalFilesAccess": false,
  "migrateEntitySetsToFolders": false,
  "store": {
    "provider": "mongodb"
  },
  "blobStorage": {
    "provider": "fs"
  },
  "templatingEngines": {
    "strategy": "in-process",
    "allowedModules": "lodash,handlebars",
    "timeout": 10000
  },
  "chrome": {
    "timeout": 30000
  },
  "loggly": {
    "level": "info",
    "subdomain": "jsreport"
  },
  "appPath": "/",
  "extensions": {
    "docker-workers": {
      "network": "nw_playground_docker_workers",
      "subnet": "182.30.0.0/24",
      "container": {
        "image": "jsreport/playground-worker:0.2.6",
        "namePrefix": "playground_worker",
        "memory": "320m",
        "memorySwap": "320m"
      }
    },
    "playground": {
      "facebook": {
        "clientID": "<fill>",
        "clientSecret": "<fill>"
      },
      "twitter": {
        "consumerKey": "<fill>",
        "consumerSecret": "<fill>"
      },
      "github": {
        "clientID": "<fill>",
        "clientSecret": "<fill>"
      }
    },
    "mongodb-store": {
      "databaseName": "playground",
      "address": "localhost"
    },
    "fs-store": {
      "syncModifications": false
    },
    "templates": {
      "studio-link-button-visibility": false
    },
    "studio": {
      "requestLogEnabled": false,
      "startupPage": false
    },
    "assets": {
      "allowAssetsLinkedToFiles": false
    },
    "authentication": {
      "cookieSession": {
        "secret": "<your strong secret>"
      },
      "admin": {
        "username": "admin",
        "password": "password"
      },
      "enabled": false
    },
    "authorization": {
      "foldersMigrationEnabled": false
    },
    "freeze": {
      "enabled": false
    },
    "reports": {
      "enabled": false
    },
    "sample-template": {
      "enabled": false
    },
    "scripts": {
      "timeout": 30000,
      "allowedModules": "http,https,lodash,bluebird,async,axios,request"
    },
    "scheduling": {
      "enabled": false
    },
    "licensing": {
      "enabled": false
    },
    "xlsx": {
      "showExcelOnlineWarning": false
    }
  }
}
