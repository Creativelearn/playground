{
    "httpPort": 5488,
    "allowLocalFilesAccess": false,
    "store": {
        "provider": "mongodb"
    },
    "blobStorage": {
        "provider": "fs"
    },
    "templatingEngines": {
        "strategy": "in-process",
        "timeout": 10000
    },
    "chrome": {
        "timeout": 30000
    },
    "appPath": "/",
    "extensions": {
        "worker-docker-manager": {
          "containerImage": "jsreport/playground-worker",
          "containerNamePrefix": "playground_worker",
          "network": "nw_playground_workers_docker_manager",
          "subnet": "182.30.0.0/24"
        },
        "playground": {
            "facebook": {
                "clientID": "<fill>",
                "clientSecret": "<fill>"
            },
            "twitter": {
                "consumerKey": "<fill>",
                "consumerSecret": "<fill>"
            },
            "github": {
                "clientID": "<fill>",
                "clientSecret": "<fill>"
            }
        },
        "mongodb-store": {
            "address": "localhost",
            "databaseName" : "playground"
        },
        "fs-store": {
           "syncModifications": false
        },
        "templates": {
            "studio-link-button-visibility": false
        },
        "authentication" : {
            "cookieSession": {
                "secret": "<your strong secret>"
            },
            "admin": {
                "username" : "admin",
                "password": "password"
            },
            "enabled": false
        },
        "freeze": {
            "enabled": false
        },
        "reports": {
            "enabled": false
        },
        "sample-template": {
            "enabled": false
        },
        "scripts": {
            "timeout": 30000
        },
        "scheduling": {
            "enabled": false
        },
        "licensing": {
            "enabled": false
        },
        "xlsx": {  
            "showExcelOnlineWarning": false          
        }
    }
}