{
  "httpPort": 5488,
  "allowLocalFilesAccess": false,
  "migrateEntitySetsToFolders": false,
  "store": {
    "provider": "mongodb"
  },
  "blobStorage": {
    "provider": "fs"
  },
  "reportTimeout": 60000,
  "encryption": {
    "enabled": false
  },
  "office": {
    "preview": {
      "showWarning": false
    }
  },
  "loggly": {
    "level": "info",
    "subdomain": "jsreport"
  },
  "sandbox": {
    "allowedModules": "handlebars,http,https,lodash,bluebird,async,axios,request,moment"
  },
  "workers": {
    "numberOfWorkers": 2
  },
  "appPath": "/",
  "extensions": {
    "docker-workers": {
      "network": "nw_playground_docker_workers",
      "subnet": "182.30.0.0/24",
      "numberOfWorkers": 2,
      "container": {
        "image": "jsreport/playground-worker:3.0.0-beta.2",
        "namePrefix": "playground_worker",
        "memory": "320m",
        "memorySwap": "320m"
      }
    },
    "playground": {
      "facebook": {
        "clientID": "<fill>",
        "clientSecret": "<fill>"
      },
      "twitter": {
        "consumerKey": "<fill>",
        "consumerSecret": "<fill>"
      },
      "github": {
        "clientID": "<fill>",
        "clientSecret": "<fill>"
      }
    },
    "mongodb-store": {
      "databaseName": "playground",
      "address": "localhost"
    },
    "studio": {
      "startupPage": false,
      "linkButtonVisibility": false
    },
    "assets": {
      "allowAssetsLinkedToFiles": false
    },
    "authentication": {
      "cookieSession": {
        "secret": "<your strong secret>"
      },
      "admin": {
        "username": "admin",
        "password": "password"
      },
      "enabled": false
    },
    "authorization": {
      "foldersMigrationEnabled": false
    },
    "freeze": {
      "enabled": false
    },
    "reports": {
      "enabled": false
    },
    "sample-template": {
      "enabled": false
    },
    "scheduling": {
      "enabled": false
    },
    "licensing": {
      "enabled": false
    }
  }
}