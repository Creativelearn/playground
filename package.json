{
  "name": "jsreport-server",
  "main": "server.js",
  "jsreport": {
    "entryPoint": "server.js"
  },
  "scripts": {
    "test": "mocha test --timeout 5000",
    "start": "jsreport-studio-start --entry-point ./server.js --httpPort 5488"
  },
  "eslintIgnore": [
    "studio/main.js",
    "studio/main.js.map"
  ],
  "standard": {
    "env": {
      "node": true,
      "browser": true,
      "mocha": true
    }
  },
  "dependencies": {
    "client-sessions": "0.8.0",
    "connect-ensure-login": "0.1.1",
    "eslint-config-standard-react": "6.0.0",
    "jsreport": "2.1.0",
    "jsreport-assets": "github:jsreport/jsreport-assets",
    "jsreport-core": "github:jsreport/jsreport-core",
    "jsreport-mongodb-store": "github:jsreport/jsreport-mongodb-store",
    "jsreport-pdf-utils": "github:jsreport/jsreport-pdf-utils",
    "jsreport-studio": "github:jsreport/jsreport-studio",
    "jsreport-version-control": "github:jsreport/jsreport-version-control",
    "jsreport-worker-docker-manager": "github:jsreport/jsreport-worker-docker-manager",
    "lodash.debounce": "4.0.8",
    "nanoid": "1.0.2",
    "passport": "0.4.0",
    "passport-facebook": "2.1.1",
    "passport-github": "1.1.0",
    "passport-twitter": "1.0.4",
    "slugify": "1.3.0"
