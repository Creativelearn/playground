{
  "name": "jsreport-playground",
  "version": "0.0.1",
  "description": "Public free playground for jsreport",
  "main": "server.js",
  "config": {
    "unsafe-perm": true
  },
  "standard": {
    "ignore": [
      "main.js",
      "examples"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "scripts": {
    "test": "standard",
    "build": "jsreport-studio-build"
  },
  "dependencies": {
    "electron": "1.6.6",
    "feedparser": "2.2.0",
    "jsreport": "1.7.0",
    "jsreport-assets": "0.6.6",
    "jsreport-ejs": "1.0.4",
    "jsreport-electron-pdf": "2.6.1",
    "jsreport-jade": "2.0.1",
    "jsreport-mongodb-store": "0.5.6",
    "jsreport-studio-dev": "1.1.0",
    "jsreport-wkhtmltopdf": "1.3.1",
    "mongodb": "2.2.26",
    "phantomjs-exact-2-1-1": "0.1.0",
    "request": "2.81.0",
    "sendgrid": "5.0.1",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "eslint": "3.19.0",
    "eslint-config-standard-jsx": "4.0.1",
    "eslint-plugin-babel": "4.1.1",
    "lodash": "4.17.4",
    "standard": "10.0.2"
  },
  "author": "Jan Blaha",
  "license": "AGPU"
}
